        <client_script><![CDATA[

api.controller=function($scope) {
	
	
	var c = this;
 // Funktion, die aufgerufen wird, wenn der Mauszeiger über ein Gebäude schwebt
  $scope.onMouseOverBuilding = function (buildingId) {
    var building = document.getElementById(buildingId);
    if (building) {
			building.style.fill = "red";
			building.style.fontSize ="4px";
    }
  };

  // Funktion, die aufgerufen wird, wenn der Mauszeiger das Gebäude verlässt
  $scope.onMouseOutBuilding = function (buildingId) {
    var building = document.getElementById(buildingId);
    if (building) {
      // Hier kannst du die Farbe auf die ursprüngliche Farbe zurücksetzen, z.B., "gray"
      building.style.fill = "white";
			building.style.fontSize ="2.414px";
    }
  };
	
	$scope.clickedOnPlace = function(id) {
				console.log("testi");
        var buildingId = id;
        

        // Senden Sie die Gebäudedaten an das Tabellen-Widget
        var myevent = new CustomEvent('buildingDataUpdated', { detail: buildingId });
        document.dispatchEvent(myevent);
    };
}

